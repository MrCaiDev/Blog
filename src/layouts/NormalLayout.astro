---
import Layout from "./Layout.astro";

export type Props = {
  title: string;
  description: string;
};
const { title, description } = Astro.props;
---

<Layout title={title} description={description}>
  <Fragment slot="og-type">
    <meta property="og:type" content="website" />
  </Fragment>
  <Fragment slot="main">
    <slot />
  </Fragment>
</Layout>
